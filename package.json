{
  "name": "react-redux-restart",
  "version": "0.0.1",
  "description": "Getting started with React and Redux",
  "main": "src/app.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "NODE_ENV=development webpack-dev-server --config webpack.config.development.babel.js",
    "dev": "NODE_ENV=development webpack-dashboard -- npm run start",
    "build": "NODE_ENV=development npm run clean && webpack --config webpack.config.production.babel.js",
    "build:watch": "NODE_ENV=development npm run build -- -w",
    "build:production": "NODE_ENV=production npm run clean && webpack --config webpack.config.production.babel.js -p",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test npm test -- --watch"
  },
  "keywords": [
    "react",
    "redux"
  ],
  "author": "Salvador Mar√≠",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.3.2",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "css-loader": "^0.26.2",
    "enzyme": "^2.7.1",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "link-media-html-webpack-plugin": "^1.0.1",
    "nock": "^9.0.9",
    "node-sass": "^4.5.0",
    "redux-devtools": "^3.3.2",
    "redux-mock-store": "^1.2.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  }
}
